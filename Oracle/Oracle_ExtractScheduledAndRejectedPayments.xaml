                </ui:GetValue.Value>
              </ui:GetValue>
              <ui:GetValue ContinueOnError="{x:Null}" DisplayName="Get Text 'Rejected Scheduled Payments'" sap2010:WorkflowViewState.IdRef="GetValue_3">
                <ui:GetValue.Target>
                  <ui:Target ClippingRegion="{x:Null}" Element="{x:Null}" TimeoutMS="{x:Null}" InformativeScreenshot="e14682878a72f67e96526776d11cbc16" Selector="[&quot;&lt;webctrl id='ResultsTable:Content' tag='TABLE' /&gt;&lt;webctrl tableCol='&quot;+in_RejectedPaymentColumnNumber.ToString+&quot;' tag='TD' /&gt;&quot;]" WaitForReady="NONE" />
                </ui:GetValue.Target>
                <ui:GetValue.Value>
                  <OutArgument x:TypeArguments="x:String">[out_RejectedPayments]</OutArgument>
                </ui:GetValue.Value>
              </ui:GetValue>
              <If Condition="[String.IsNullOrEmpty(out_RejectedPayments)]" DisplayName="If Rejected Payment is empty" sap2010:WorkflowViewState.IdRef="If_1">
                <If.Then>
                  <Assign DisplayName="Assign Rejected payment to 0" sap2010:WorkflowViewState.IdRef="Assign_4">
                    <Assign.To>
                      <OutArgument x:TypeArguments="x:String">[out_RejectedPayments]</OutArgument>
                    </Assign.To>
                    <Assign.Value>
                      <InArgument x:TypeArguments="x:String">[in_ZeroValue]</InArgument>
                    </Assign.Value>
                  </Assign>
                </If.Then>
              </If>
              <Assign DisplayName="Assign Total Payments" sap2010:WorkflowViewState.IdRef="Assign_3">
                <Assign.To>
                  <OutArgument x:TypeArguments="x:Int32">[out_TotalPayments]</OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="x:Int32">[Convert.ToInt32(out_ScheduledPayments) + Convert.ToInt32(out_RejectedPayments)]</InArgument>
                </Assign.Value>
              </Assign>
              <ui:CloseTab Browser="[PPRWindow]" DisplayName="Close Tab" sap2010:WorkflowViewState.IdRef="CloseTab_2" />
            </Sequence>
          </Sequence>
        </ActivityAction>
      </ui:BrowserScope.Body>
    </ui:BrowserScope>
