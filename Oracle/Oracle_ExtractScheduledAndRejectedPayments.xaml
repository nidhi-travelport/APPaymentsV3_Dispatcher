                  <ui:Target ClippingRegion="{x:Null}" Element="{x:Null}" TimeoutMS="{x:Null}" InformativeScreenshot="6c331f22e1ffe2ef8e3c3d2c07d25f9d" Selector="[&quot;&lt;webctrl id='ResultsTable:Content' tag='TABLE' /&gt;&lt;webctrl tableCol='&quot;+in_SelectedPaymentColumnNumber.ToString+&quot;' tag='TD' /&gt;&quot;]" WaitForReady="NONE" />
                </ui:GetValue.Target>
                <ui:GetValue.Value>
